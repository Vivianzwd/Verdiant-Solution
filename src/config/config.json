{
  "site": {
    "title": "DataTraces",
    "base_url": "https://datatraces.netlify.app/",
    "base_path": "/",
    "trailing_slash": false,
    "favicon": "/images/favicon.png",
    "logo": "/images/datatraces_logo.png",
    "logo_darkmode": "/images/datatraces_dark_logo.png",
    "logo_width": "150",
    "logo_height": "63",
    "logo_text": "DataTraces"
  },

  "settings": {
    "search": true,
    "sticky_header": true,
    "theme_switcher": true,
    "default_theme": "system",
    "pagination": 2,
    "summary_length": 200,
    "blog_folder": "blog"
  },

  "params": {
    "contact_form_action": "#",
    "copyright": "Template by [Zeon Studio](https://zeon.studio)"
  },

  "navigation_button": {
    "enable": true,
    "label": "Repo",
    "link": "https://github.com/minghao51/datatraces"
  },

  "disqus": {
    "enable": true,
    "shortname": "themefisher-template",
    "settings": {}
  },

  "metadata": {
    "meta_author": "howt51",
    "meta_image": "/images/og-image.png",
    "meta_description": "blog via astro boilerplate"
  }
}
