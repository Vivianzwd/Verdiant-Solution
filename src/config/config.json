{
  "site": {
    "title": "Verdiant",
    "base_url": "https://astroplate.netlify.app",
    "base_path": "/",
    "trailing_slash": false,
    "favicon": "/images/verdiant_logo_original.png",
    "logo": "/images/verdiant_earth_logo_3.png",
    "logo_darkmode": "/images/logo-darkmode.png",
    "logo_width": "200",
    "logo_height": "40",
    "logo_text": "Verdiant"
  },

  "settings": {
    "search": true,
    "sticky_header": true,
    "theme_switcher": false,
    "default_theme": "system",
    "pagination": 2,
    "summary_length": 200,
    "blog_folder": "blog"
  },

  "params": {
    "contact_form_action": "#",
    "copyright": "Template by [Zeon Studio](https://zeon.studio)"
  },

  "navigation_button": {
    "enable": true,
    "label": "Book an appointment",
    "link": "/contact"
  },

  "disqus": {
    "enable": true,
    "shortname": "themefisher-template",
    "settings": {}
  },

  "metadata": {
    "meta_author": "Vivian",
    "meta_image": "/images/og-image.png",
    "meta_description": "blog via astro boilerplate"
  }
}
